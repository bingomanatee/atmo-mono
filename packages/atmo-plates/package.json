{
  "name": "@wonderlandlabs/atmo-plates",
  "version": "0.2.0",
  "description": "A plate tectonics simulation library",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "build:all": "yarn build && yarn build:types",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "build:types": "node scripts/generate-dts.js",
    "prepublishOnly": "yarn build:all"
  },
  "devDependencies": {
    "@types/lodash-es": "^4",
    "@types/three": "^0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^3.1.3"
  },
  "dependencies": {
    "@wonderlandlabs/atmo-utils": "workspace:*",
    "@wonderlandlabs/multiverse": "workspace:*",
    "dexie": "^4.0.11",
    "h3-js": "^4.2.1",
    "idb": "^8.0.0",
    "lodash-es": "^4.17.21",
    "rxjs": "^7.8.1",
    "three": "^0.176.0",
    "uuid": "^11.1.0"
  },
  "keywords": [
    "plate tectonics",
    "simulation",
    "geology",
    "3D"
  ],
  "sideEffects": false
}
